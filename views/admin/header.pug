body 

  // Page Wrapper 
  #wrapper
    // Sidebar
    ul#accordionSidebar.navbar-nav.bg-gradient-primary.sidebar.sidebar-dark.accordion
      // Sidebar - Brand
      a.sidebar-brand.d-flex.align-items-center.justify-content-center(href='/admin')
          <i class="fas fa-user-alt"></i>
          .sidebar-brand-text.mx-3
              | Admin 
      // Divider
      hr.sidebar-divider.my-0
      // Nav Item - Dashboard
      li.nav-item.active
        a.nav-link(href='/admin/')
          i.fas.fa-fw.fa-tachometer-alt
          span Dashboard
      // Divider
      hr.sidebar-divider
      // Heading
      .sidebar-heading
        | Functions
      // Nav Item - Pages Collapse Menu
      li.nav-item
        a.nav-link.collapsed(href='/admin/addProduct', aria-expanded='true')
          <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" class="bi bi-plus-square" viewBox="0 0 16 16">
